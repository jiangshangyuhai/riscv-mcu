{
    "files.associations": {
        "*.ejs": "html",
        "*.h": "c",
        "*.v": "verilog",
        "*.s": "assembly"
    },
    "verilog.linting.linter": "iverilog",
    "verilog.linting.iverilog.includePath": ["verilog","verilog/core","verilog/soc"],
    "verilog.linting.iverilog.arguments": "-yverilog -yverilog/peripherals/digital -yverilog/core -yverilog/soc -yverilog/peripherals",
    "verilog.linting.iverilog.runAtFileLocation": false
}